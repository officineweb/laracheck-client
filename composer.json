{
  "name": "officineweb/laracheck-client",
  "description": "Laracheck Client - Track exceptions and errors in your Laravel applications",
  "keywords": [
    "laracheck",
    "laravel",
    "exception",
    "error",
    "tracking",
    "monitoring",
    "bug",
    "404"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Officine Web",
      "email": "info@officineweb.it"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Laracheck\\Client\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laracheck\\Client\\LaracheckServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
